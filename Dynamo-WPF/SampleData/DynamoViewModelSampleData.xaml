<vm:DynamoViewModel xmlns:vm="clr-namespace:Dynamo.UI.Wpf.ViewModels"
                    xmlns:models="clr-namespace:Dynamo.UI.Models;assembly=DynamoModels">
    <vm:DynamoViewModel.Workspaces>
        <vm:WorkspaceViewModel Name="Workspace 1"/>
        <vm:WorkspaceViewModel Name="Workspace 2"/>
        <vm:WorkspaceViewModel Name="Workspace 3"/>
    </vm:DynamoViewModel.Workspaces>
    <vm:DynamoViewModel.Sidebars>
        <vm:SampleConsoleViewModel>
            <vm:SampleConsoleViewModel.SampleEntries>
                <models:LogEntry Text="Welcome to Dynamo!"/>
                <models:LogEntry Severity="Warning" Text="Be careful..."/>
                <models:LogEntry Severity="Hint" Text="Nodes can be passed as functions."/>
                <models:LogEntry Severity="Error" Text="You done messed up!"/>
            </vm:SampleConsoleViewModel.SampleEntries>
        </vm:SampleConsoleViewModel>
    </vm:DynamoViewModel.Sidebars>
</vm:DynamoViewModel>
